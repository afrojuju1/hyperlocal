# OpenAI (only needed if HYPERLOCAL_IMAGE_PROVIDER=openai)
OPENAI_API_KEY=your_key_here
# OPENAI_BASE_URL=http://localhost:8000/v1

# LLM provider (vllm-mlx recommended)
# HYPERLOCAL_LLM_PROVIDER=vllm_mlx
# HYPERLOCAL_LLM_API_KEY=not-needed
# HYPERLOCAL_TEXT_BASE_URL=http://localhost:11435/v1
# HYPERLOCAL_VISION_BASE_URL=http://localhost:11436/v1
# HYPERLOCAL_TEXT_MODEL=default
# HYPERLOCAL_VISION_MODEL=default

# Ollama (legacy, OpenAI-compatible)
# OLLAMA_BASE_URL=http://localhost:11434/v1
# OLLAMA_API_KEY=ollama

# Image generation (local SDXL)
# HYPERLOCAL_IMAGE_PROVIDER=sdxl
# SDXL_API_URL=http://localhost:17860/sdapi/v1/txt2img
# SDXL_STEPS=2
# SDXL_CFG_SCALE=0
# SDXL_SAMPLER=Euler a

# Image generation (ComfyUI)
# HYPERLOCAL_IMAGE_PROVIDER=comfyui
# COMFYUI_API_URL=http://localhost:8188
# COMFYUI_WORKFLOW_PATH=comfyui/workflows/flyer_full.json
# COMFYUI_TIMEOUT=600
# COMFYUI_OUTPUT_NODE=

# Database
# DATABASE_URL=postgresql+psycopg://hyperlocal:hyperlocal@localhost:55432/hyperlocal

# Optional overrides
# HYPERLOCAL_VISION_MODEL=llama3.2-vision:latest
# HYPERLOCAL_TEXT_MODEL=qwen2.5:7b
# HYPERLOCAL_IMAGE_MODEL=gpt-image-1
# HYPERLOCAL_IMAGE_SIZE=1024x1536
# HYPERLOCAL_IMAGE_QUALITY=high
# HYPERLOCAL_QC_ENABLED=0
# HYPERLOCAL_VARIANTS=1
# HYPERLOCAL_PERSIST_ENABLED=0
# TYPST_BIN=typst
